#!/bin/bash
set -e

echo '-----> [$1, $2, $3]'
echo '[' "'$1'" ',' "'$2'" ',' "'$3'" ']'

echo '-----> pwd'
pwd

echo '-----> ls -ahl'
ls -ahl

echo '-----> cd .io.livecode.ch/'
cd .io.livecode.ch/

echo '-----> tar'
tar -xvzf ./hello-1.0.0-linux-x86_64.tar.gz

echo '-----> cd'
cd ./hello-1.0.0-linux-x86_64/

echo '-----> hello'
./hello
